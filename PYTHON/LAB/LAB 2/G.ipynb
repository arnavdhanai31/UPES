   "source": [
    "<i><h4>Exchange values of 2 numbers without using a temporary variable</h4></i>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "t=int(input(\"Enter a number: \"))\n",
    "t2=int(input(\"Enter a number: \"))\n",
    "print(\"Before swapping\\nT: \",t,\" T2: \",t2)\n",
    "t=t+t2\n",
    "t2=t-t2\n",
    "t=t-t2\n",
    "print(\"After swapping\\nT: \",t,\" T2: \",t2)"
   ]
  }
